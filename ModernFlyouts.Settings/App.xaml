                        <ResourceDictionary>
                            <Color x:Key="RegionColor">#FF000000</Color>

                            <SolidColorBrush x:Key="HvidSortBrush" Color="White" />
                            <SolidColorBrush x:Key="BlåBrush" Color="#1dd1ec" />
                            <SolidColorBrush x:Key="RødBrush" Color="#ff6363" />
                            <SolidColorBrush x:Key="GrønBrush" Color="#1dec3c" />
                            <Color x:Key="HvidSortColor">White</Color>
                            <SolidColorBrush x:Key="SortHvidBrush" Color="Black" />
                            <SolidColorBrush x:Key="11Brush" Color="#111111" />
                            <SolidColorBrush x:Key="12Brush" Color="#121212" />
                            <SolidColorBrush x:Key="1cBrush" Color="#1c1c1c" />
                            <SolidColorBrush x:Key="24Brush" Color="#242424" />
                            <SolidColorBrush x:Key="FABrush" Color="#1c1c1c"/>
                            <SolidColorBrush x:Key="F2Brush" Color="#111111"/>
                            <SolidColorBrush x:Key="BrushForSearch" Color="#282828"/>

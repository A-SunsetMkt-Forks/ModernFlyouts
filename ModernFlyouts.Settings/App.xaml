                        <ResourceDictionary>
                            <SolidColorBrush x:Key="SortHvidBrush" Color="White" />
                            <SolidColorBrush x:Key="BlåBrush" Color="#3f48cc" />
                            <SolidColorBrush x:Key="RødBrush" Color="#cc3f3f" />
                            <SolidColorBrush x:Key="GrønBrush" Color="#438c4e" />
                            <Color x:Key="HvidSortColor">Black</Color>
                            <Color x:Key="SystemRevealListLowColor">#FFE6E6E6</Color>
                            <Color x:Key="SystemRevealListMediumColor">#FFCCCCCC</Color>
                            <SolidColorBrush x:Key="11Brush" Color="#eeeeee" />
                            <SolidColorBrush x:Key="12Brush" Color="#ededed" />
                            <SolidColorBrush x:Key="1cBrush" Color="#e3e3e3" />
                            <SolidColorBrush x:Key="24Brush" Color="#dbdbdb" />
                            <SolidColorBrush x:Key="FABrush" Color="#FAFAFA"/>
                            <SolidColorBrush x:Key="F2Brush" Color="#F2F2F2"/>
                            <SolidColorBrush x:Key="BrushForSearch" Color="#FFFFFF"/>
                            <SolidColorBrush x:Key="ResponsiveAccentBrush3" Color="{ThemeResource SystemAccentColorDark3}"/>
                            <SolidColorBrush x:Key="ResponsiveAccentBrush2" Color="{ThemeResource SystemAccentColorDark2}"/>
                            <SolidColorBrush x:Key="ResponsiveAccentBrush1" Color="{ThemeResource SystemAccentColorDark1}"/>
                            <SolidColorBrush x:Key="HvidSortBrush" Color="Black" />
                            <RevealBackgroundBrush x:Key="SystemControlHighlightListLowRevealBackgroundBrush" TargetTheme="Light" Color="{ThemeResource SystemRevealListMediumColor}" FallbackColor="{ StaticResource SystemListMediumColor}" />
                            <Color x:Key="RegionColor">#FFFFFFFF</Color>
                            <SolidColorBrush x:Key="RegionBrush" Color="{StaticResource RegionColor}" />
                            <SolidColorBrush x:Key="BorderHighContrastBrush" Color="Transparent" />



                            <AcrylicBrush x:Key="PageBackground" BackgroundSource="HostBackdrop" FallbackColor="#dbdbdb" TintColor="#dbdbdb" TintOpacity="0.80"/>
                        </ResourceDictionary>
                    </ResourceDictionary.MergedDictionaries>
                </ResourceDictionary>
                <ResourceDictionary x:Key="HighContrast">
                    <StaticResource x:Key="RegionColor" ResourceKey="SystemColorWindowColor" />
                    <SolidColorBrush x:Key="RegionBrush" Color="{StaticResource RegionColor}" />
